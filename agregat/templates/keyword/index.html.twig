{% extends 'base.html.twig' %}
{% block title %}Products index{% endblock %}

{% block body %}
    <h1>Keywords index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>KeyWords</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for product in products %}
            <tr>
                <td>{{ product.keyWords }}</td>
                <td>
                    <a href="{{ path('app_keyword_show', {'id': product.id}) }}">show</a>
                    <a href="{{ path('app_keyword_edit', {'id': product.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="18">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
